    "docs:serve": "cd docs && jekyll serve & open http://localhost:4000",
    "docs:tooltip": "rollup -c ./scripts/rollup.config.tooltip.js -o ./.tmp/tooltip.js && jsdoc2md ./.tmp/tooltip.js > ./docs/_includes/tooltip-documentation.md",
